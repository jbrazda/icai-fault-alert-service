                <Type>alert-email</Type>
            </action>
            <condition>
                <guid>159ccd35-f8a0-433e-82ca-7dea2df10f3e</guid>
                <type>equals</type>
                <value/>
                <not>true</not>
                <alert-field>processName</alert-field>
            </condition>
        </rule>
    </rules>
